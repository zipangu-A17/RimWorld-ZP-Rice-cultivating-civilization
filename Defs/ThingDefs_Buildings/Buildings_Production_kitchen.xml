<?xml version="1.0" encoding="utf-8"?>
<ThingDefs>
	<ThingDef ParentName="Rice_BuildingBase">
		<defName>Messtin</defName>
		<label>飯盒</label>
		<ThingClass>Building_WorkTable_HeatPush</ThingClass>
		<Description>ご飯を炊いたり簡単な調理をするのに使われる。\n最低限の設備しかないため調理速度が30%低下する。</Description>
		<graphicData>
			<texPath>Things/Buildings/Messtin/Messtin</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1.3,1.3)</drawSize>
			<drawRotated>false</drawRotated>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<constructEffect>ConstructMetal</constructEffect>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<CostList>
			<Steel>10</Steel>
			<WoodLog>10</WoodLog>
		</CostList>
		<AltitudeLayer>Building</AltitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>500</WorkToBuild>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<WorkTableWorkSpeedFactor>0.7</WorkTableWorkSpeedFactor>
		</statBases>
		<Size>(1,1)</Size>
		<minifiedDef>MinifiedFurniture</minifiedDef>
		<Passability>PassThroughOnly</Passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<recipes>
			<!--== 調味料 ==-->
			<li>R_CookNaturalSalt</li>
			<li>R_CookSyrup10</li>
			<!--== バニラ ==-->
			<li>CookMealSimple</li>
			<!--== 玄米 ==-->
			<li>CookBRGohan1</li>
			<li>CookBRGohan4</li>
			<li>CookBRTKG1</li>
			<li>CookBRTKG4</li>
			<li>CookBRGyudon1</li>
			<li>CookBRGyudon4</li>
			<li>CookBROnigiri4</li>
			<!--== 白米 ==-->
			<li>CookGohan1</li>
			<li>CookGohan4</li>
			<li>CookWRTKG1</li>
			<li>CookWRTKG4</li>
			<li>CookWRGyudon1</li>
			<li>CookWRGyudon4</li>
			<li>CookWROnigiri4</li>
		</recipes>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<comps>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>160.0</fuelConsumptionRate>
				<fuelCapacity>50.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>WoodLog</li>
						<li>Momigalite</li>
						<li>FuelStraw</li>
					</thingDefs>
				</fuelFilter>
				<consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
			</li>
		</comps>
		<building>
			<isMealSource>true</isMealSource>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<heatPerTickWhileWorking>0.10</heatPerTickWhileWorking>
		</building>
		<DesignationCategory>Zipangu</DesignationCategory>
	</ThingDef>
