	<!--=============== マルチ式脱穀精米機　稲穂⇒白米 ====================-->
	<RecipeDef>
		<defName>MakeThreshingRiceMillingMachine10</defName>
		<label>稲穂を白米にする(10)</label>
		<description>稲穂を脱穀精米して白米、藁、籾殻、米ぬかにわける</description>
		<jobString>脱穀精米中</jobString>
		<workAmount>400</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Inaho</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<WhiteRice>15</WhiteRice>
			<Straw>10</Straw>
			<Chaff>10</Chaff>
			<RiceBran>10</RiceBran>
		</products>
		<skillRequirements>
			<li>
				<skill>Growing</skill>
				<minLevel>4</minLevel>
			</li>
		</skillRequirements>
		<fixedIngredientFilter>
			<exceptedCategories>
				<li>AnimalProductRaw</li>
				<li>MeatRaw</li>
				<li>PlantFoodRaw</li>
			</exceptedCategories>
			<thingDefs>
				<li>Inaho</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Growing</workSkill>
	</RecipeDef>
