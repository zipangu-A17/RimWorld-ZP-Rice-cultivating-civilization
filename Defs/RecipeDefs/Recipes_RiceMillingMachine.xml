	<!--=============== 精米機　米(籾)⇒白米 ====================-->
	<RecipeDef>
		<defName>MakeRiceMillingMachineRice10</defName>
		<label>米(籾)を白米にする(10)</label>
		<description>米(籾)を精米して白米、籾殻、米ぬかにわける。</description>
		<jobString>精米中</jobString>
		<workAmount>400</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RiceMomi</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<WhiteRice>10</WhiteRice>
			<Chaff>10</Chaff>
			<RiceBran>10</RiceBran>
		</products>
		<fixedIngredientFilter>
			<exceptedCategories>
				<li>AnimalProductRaw</li>
				<li>MeatRaw</li>
				<li>PlantFoodRaw</li>
			</exceptedCategories>
			<thingDefs>
				<li>RiceMomi</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workSkill>Growing</workSkill>
	</RecipeDef>
