		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BrownRice</li>
					</thingDefs>
				</filter>
				<count>0.25</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>0.25</count>
			</li>
		</ingredients>
		<products>
			<BR_Gyudon>1</BR_Gyudon>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BrownRice</li>
			</thingDefs>
			<categories>
				<li>MeatRaw</li>
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FoodRaw</li>
			</categories>
			<exceptedThingDefs>
				<li>Human_Meat</li>
				<li>Megaspider_Meat</li>
			</exceptedThingDefs>
		</defaultIngredientFilter>
	</RecipeDef>
