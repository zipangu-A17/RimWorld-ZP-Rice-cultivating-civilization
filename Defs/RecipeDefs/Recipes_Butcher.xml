<RecipeDefs>
	<RecipeDef>
		<defName>MakeR_SeasonedMeat5</defName>
		<label>味付け肉(5)</label>
		<description>調味料で臭みを消し栄養素と保存性を少し高めた味付け肉を作る。</description>
		<jobString>味付け肉を作る</jobString>
		<workAmount>400</workAmount>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>TableButcher</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>Seasoning</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<R_SeasonedMeat>5</R_SeasonedMeat>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>Seasoning</li>
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FoodRaw</li>
			</categories>
			<exceptedThingDefs>
				<li>R_Syrup</li>
